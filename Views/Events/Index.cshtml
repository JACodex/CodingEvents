@{
    <h2>@ViewBag.Name</h2>
    <ol>
        @foreach (string food in ViewBag.Foods)
        {
            <li>@food</li>
        }
    </ol>
    <a asp-controller="Events" asp-action="FormPost">Add Name</a>
}
